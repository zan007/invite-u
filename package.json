{
  "name": "invite-u",
  "version": "0.0.1",
  "description": "",
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "webpack-serve",
    "build:dev": "webpack --progress",
    "build:prod": "rimraf ./dist && webpack -p --env production",
    "serve:dist": "http-server ./dist -p 4000 -c-1",
    "profile": "webpack --profile --json > webpack-stats.json",
    "lint:css": "stylelint \"./src/**/*.scss\" --fix",
    "lint:css:strict": "stylelint \"./src/**/*.scss\"",
    "lint:ts": "tslint -c ./tslint.json -p ./tsconfig.json -t codeFrame -e node_modules",
    "test:e2e": "node ./tests/e2e/index.js",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "heroku-postbuild": "npm run build:prod"
  },
  "engines": {
    "node": "8.10.0"
  },
  "author": "pploszaj",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.5",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.0",
    "@types/googlemaps": "^3.30.13",
    "@types/react": "^16.0.26",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.19",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-transition-group": "^2.0.14",
    "@types/redux-logger": "^3.0.5",
    "@types/smoothscroll-polyfill": "^0.3.0",
    "@types/snazzy-info-window": "^1.1.3",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.2.0",
    "googlemaps": "^1.12.0",
    "google-maps-api-loader": "^1.1.1",
    "snazzy-info-window": "^1.1.1",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.0.4",
    "http-server": "^0.11.1",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.0",
    "ts-loader": "^4.0.0",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "scriptjs": "^2.5.9",
    "webfonts-generator": "^0.4.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.13",
    "webpack-serve": "0.3.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "contentful": "^5.1.3",
    "foundation-sites": "^6.4.4-rc1",
    "express": "^4.17.3",
    "immutable": "^3.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-id-swiper": "^1.6.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.5.0",
    "react-markdown": "^3.6.0",
    "uuid": "^3.3.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "scrollmonitor": "^1.2.4",
    "smoothscroll-polyfill": "^0.4.3"
  },
  "pre-commit": [
    "lint:css",
    "lint:ts"
  ],
  "jest": {
    "setupFiles": [
      "./tests/unit/jest-config.js",
      "./tests/unit/mocks.js"
    ],
    "modulePaths": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/unit/preprocessor.js"
    },
    "testRegex": "(/__tests__/.+\\.(spec|test))\\.(ts|tsx|js)$",
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/unit/file-mock.js",
      "^.+\\.(css|scss)$": "<rootDir>/tests/unit/style-mock.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "globals": {
      "__CONTENTFUL_SPACE_ID__": "__CONTENTFUL_SPACE_ID__",
      "__CONTENTFUL_ACCESS_TOKEN__": "__CONTENTFUL_ACCESS_TOKEN__",
      "__GOOGLE_MAPS_API_KEY__": "__GOOGLE_MAPS_API_KEY__"
    }
  }
}
